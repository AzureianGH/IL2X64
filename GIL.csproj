<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <PublishAot>False</PublishAot>
    <Title>GunnerIL</Title>
    <Company>Aquanite</Company>
    <Description>A library/program that allows quick decompilation of IL code.</Description>
    <RepositoryUrl>https://github.com/AzureianGH/GunnerIL</RepositoryUrl>
    <PackAsTool>True</PackAsTool>
    <ToolCommandName>gil</ToolCommandName>
  </PropertyGroup>

</Project>
